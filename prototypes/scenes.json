{
	"config": {},
	"libraries": [
		"../prototypes/js/three.js"
	],
	"includes": [
		[
			"Renderer",
			[
				"var dom = resources.get( 'dom' );",
				"",
				"var renderer = new THREE.WebGLRenderer( { antialias: true } );",
				"renderer.setPixelRatio( window.devicePixelRatio );",
				"renderer.setSize( dom.clientWidth, dom.clientHeight );",
				"renderer.shadowMap.enabled = true;",
				"renderer.gammaInput = true;",
				"renderer.gammaOutput = true;",
				"dom.appendChild( renderer.domElement );",
				"",
				"window.addEventListener( 'resize', function () {",
				"\t",
				"\trenderer.setSize( dom.clientWidth, dom.clientHeight );",
				"\t",
				"} );",
				"",
				"resources.set( 'renderer', renderer );"
			]
		],
		[
			"Scene",
			[
				"var scene = new THREE.Scene();",
				"scene.background = new THREE.Color( 0xE8E8E8 );",
				"",
				"var ambient = new THREE.HemisphereLight( 0xFEFCFF, 0xB0ADAA, 0.9 );",
				"scene.add( ambient );",
				"",
				"var light = new THREE.DirectionalLight( 0xFFFFFF, 0.35 );",
				"light.position.set( 2.5, 5.0, 2.5 );",
				"light.castShadow = true;",
				"light.shadow.radius = 10;",
				"scene.add( light );",
				"",
				"resources.set( 'scene', scene );"
			]
		],
		[
			"Camera",
			[
				"var dom = resources.get( 'dom' );",
				"",
				"var camera = new THREE.PerspectiveCamera( 40, dom.clientWidth / dom.clientHeight, 0.1, 100 );",
				"",
				"window.addEventListener( 'resize', function () {",
				"\tcamera.aspect = dom.clientWidth / dom.clientHeight;",
				"\tcamera.updateProjectionMatrix();",
				"} );",
				"",
				"resources.set( 'camera', camera );"
			]
		],
		[
			"Assets",
			[
				"function load( url ) {",
				"",
				"\tvar img = document.createElement( 'img' );",
				"\timg.src = url;",
				"\treturn img;",
				"",
				"}",
				"",
				"resources.set( 'assets/images/matcap', load( '../prototypes/textures/matcap.jpg' ) );",
				"resources.set( 'assets/images/noise1', load( '../prototypes/textures/noise.png' ) );",
				"resources.set( 'assets/images/noise2', load( '../prototypes/textures/noise2.png' ) );"
			]
		]
	],
	"effects": [
		[
			"Scene 1",
			[
				"var scene = resources.get( 'scene' );",
				"",
				"var group = new THREE.Group();",
				"",
				"// SPHERE 1",
				"",
				"var texture1 = new THREE.Texture( resources.get( 'assets/images/noise2' ) );",
				"texture1.anisotropy = 16;",
				"texture1.needsUpdate = true;",
				"",
				"var texture2 = new THREE.Texture( resources.get( 'assets/images/noise1' ) );",
				"texture2.anisotropy = 16;",
				"texture2.needsUpdate = true;",
				"",
				"var texture3 = new THREE.Texture( resources.get( 'assets/images/matcap' ) );",
				"texture3.mapping = THREE.SphericalReflectionMapping;",
				"texture3.needsUpdate = true;",
				"",
				"var mesh = new THREE.Mesh(",
				"\tnew THREE.SphereBufferGeometry( 1, 64, 32 ),",
				"\tnew THREE.MeshStandardMaterial( {",
				"\t\tcolor: 0x161EA2,",
				"\t\troughness: 1.0,",
				"\t\tmetalness: 1.0,",
				"\t\troughnessMap: texture1,",
				"\t\tmetalMap: texture2,",
				"\t\tenvMap: texture3",
				"\t} )",
				");",
				"mesh.position.x = - 1.35;",
				"mesh.position.y = 2.0;",
				"mesh.castShadow = true;",
				"mesh.receiveShadow = true;",
				"group.add( mesh );",
				"",
				"// SPHERE 2",
				"",
				"var texture = new THREE.Texture( resources.get( 'assets/images/noise1' ) );",
				"texture.anisotropy = 16;",
				"texture.needsUpdate = true;",
				"",
				"var mesh = new THREE.Mesh(",
				"\tnew THREE.SphereBufferGeometry( 1, 64, 32 ),",
				"\tnew THREE.MeshStandardMaterial( {",
				"\t\tcolor: 0xD4D4D4,",
				"\t\tmap: texture,",
				"\t\troughness: 1.0,",
				"\t\tmetalness: 0.0",
				"\t} )",
				");",
				"mesh.position.x = 1.35;",
				"mesh.position.y = 2.0;",
				"mesh.castShadow = true;",
				"mesh.receiveShadow = true;",
				"group.add( mesh );",
				"",
				"// FLOOR",
				"",
				"var texture = new THREE.Texture( resources.get( 'assets/images/noise1' ) );",
				"texture.anisotropy = 16;",
				"texture.wrapS = THREE.RepeatWrapping;",
				"texture.wrapT = THREE.RepeatWrapping;",
				"texture.repeat.set( 2, 2 );",
				"texture.needsUpdate = true;",
				"",
				"var floor = new THREE.Mesh(",
				"\tnew THREE.PlaneBufferGeometry( 20, 20 ),",
				"\tnew THREE.MeshStandardMaterial( {",
				"\t\tcolor: 0xD4D4D4,",
				"\t\tmap: texture,",
				"\t\troughness: 1.0,",
				"\t\tmetalness: 0.0",
				"\t} )",
				");",
				"floor.rotation.x = - Math.PI / 2;",
				"floor.receiveShadow = true;",
				"group.add( floor );",
				"",
				"//",
				"",
				"function start( progress ) {",
				"",
				"\tscene.add( group );",
				"",
				"}",
				"",
				"function end( progress ) {",
				"\t",
				"\tscene.remove( group );",
				"\t",
				"}",
				"",
				"function update( progress ) {",
				"",
				"\t// mesh.rotation.x = progress * Math.PI * 2;",
				"",
				"}"
			]
		],
		[
			"Orbit Camera",
			[
				"var camera = resources.get( 'camera' );",
				"var target = new THREE.Vector3( 0, 2.0, 0 );",
				"",
				"function start( progress ){}",
				"",
				"function end( progress ){}",
				"",
				"function update( progress ){",
				"\t",
				"\tcamera.position.y = 2.0;",
				"\tcamera.position.x = Math.cos( progress * Math.PI * 2.0 ) * Math.PI * 1.5;",
				"\tcamera.position.z = Math.sin( progress * Math.PI * 2.0 ) * Math.PI * 1.5;",
				"\tcamera.lookAt( target );",
				"",
				"}"
			]
		],
		[
			"Render",
			[
				"var renderer = resources.get( 'renderer' );",
				"var scene = resources.get( 'scene' );",
				"var camera = resources.get( 'camera' );",
				"",
				"function start( progress ){}",
				"",
				"function end( progress ){}",
				"",
				"function update( progress ){",
				"",
				"\trenderer.render( scene, camera );",
				"",
				"}"
			]
		],
		[
			"Audio",
			[
				"var audio = document.createElement( 'audio' );",
				"audio.src = '../assets/What-You-Dont-Know-1C-RX6.mp3';",
				"",
				"function start( progress ){",
				"",
				"\tplayer.setAudio( audio );",
				"",
				"}",
				"",
				"function end( progress ){",
				"",
				"\tplayer.setAudio( null );",
				"",
				"}",
				"",
				"function update( progress ){}"
			]
		],
		[
			"Scene 2",
			[
				"var image = resources.get( 'assets/images/noise1' );",
				"",
				"var scene = resources.get( 'scene' );",
				"",
				"var group = new THREE.Group();",
				"",
				"// FLOOR",
				"",
				"var texture = new THREE.Texture( image );",
				"texture.anisotropy = 16;",
				"texture.wrapS = THREE.RepeatWrapping;",
				"texture.wrapT = THREE.RepeatWrapping;",
				"texture.repeat.set( 4, 4 );",
				"texture.needsUpdate = true;",
				"",
				"var floor = new THREE.Mesh(",
				"\tnew THREE.PlaneBufferGeometry( 40, 40 ),",
				"\tnew THREE.MeshStandardMaterial( {",
				"\t\tcolor: 0xD4D4D4,",
				"\t\tmap: texture,",
				"\t\troughness: 1.0,",
				"\t\tmetalness: 0.0",
				"\t} )",
				");",
				"floor.rotation.x = - Math.PI / 2;",
				"floor.receiveShadow = true;",
				"group.add( floor );",
				"",
				"// TUBES",
				"",
				"var texture = new THREE.Texture( image );",
				"texture.anisotropy = 16;",
				"texture.repeat.set( 0.4, 0.6 );",
				"texture.needsUpdate = true;",
				"",
				"var geometry = new THREE.CylinderBufferGeometry( 0.2, 0.2, 1, 32 );",
				"geometry.translate( 0, 0.5, 0 );",
				"",
				"var materials = [",
				"\tnew THREE.MeshStandardMaterial( {",
				"\t\tcolor: 0xD4D4D4,",
				"\t\tmap: texture,",
				"\t\troughness: 0.8,",
				"\t\tmetalness: 0.0",
				"\t} ),",
				"\tnew THREE.MeshStandardMaterial( {",
				"\t\tcolor: 0x404040,",
				"\t\tmap: texture,",
				"\t\troughness: 0.8,",
				"\t\tmetalness: 0.0",
				"\t} )",
				"];",
				"",
				"for ( var x = 0; x < 10; x ++ ) {",
				"",
				"\tfor ( var z = 0; z < 10; z ++ ) {",
				"",
				"\t\tvar clone = new THREE.Mesh( geometry, materials[ x % 2 ] );",
				"\t\tclone.position.x = x - 5;",
				"\t\tclone.position.z = z - 5;",
				"\t\tclone.castShadow = true;",
				"\t\tclone.receiveShadow = true;",
				"\t\tgroup.add( clone );",
				"\t\t",
				"\t}",
				"",
				"}",
				"",
				"//",
				"",
				"function start( progress ) {",
				"",
				"\tscene.add( group );",
				"",
				"}",
				"",
				"function end( progress ) {",
				"\t",
				"\tscene.remove( group );",
				"\t",
				"}",
				"",
				"function update( progress ) {",
				"",
				"\tfor ( var i = 1; i < group.children.length; i ++ ) {",
				"\t\t",
				"\t\tvar child = group.children[ i ];",
				"",
				"\t\tvar x = ( i % 10 ) + progress * 20.0;",
				"\t\tvar y = Math.abs( i / 10 ) + progress * 20.0;",
				"",
				"\t\tchild.scale.y = Math.abs( Math.sin( x ) + Math.cos( y ) ) + 0.1;",
				"",
				"\t}",
				"",
				"}"
			]
		]
	],
