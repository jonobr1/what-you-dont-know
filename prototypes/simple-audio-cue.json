				"",
				"\t\tvar sid = intensity[ i ];",
				"\t\tvar section = sections[ sid ];",
				"\t\tvar url = [",
				"\t\t\t'../assets/loops/',",
				"\t\t\tf.toUpperCase(),",
				"\t\t\t'-',",
				"\t\t\tsection,",
				"\t\t\t'.',",
				"\t\t\tformats.standard // TODO: Check for OSX",
